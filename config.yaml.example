# Конфигурация Wialon Billing API

server:
  port: "8080"

database:
  host: "localhost"
  port: "5432"
  user: "postgres"
  password: "postgres"
  dbname: "wialon_billing"
  sslmode: "disable"

wialon:
  # Для Wialon Hosting: https://hst-api.wialon.com
  # Для Wialon Local: https://your-server.com
  base_url: "https://hst-api.wialon.com"
  token: "YOUR_WIALON_TOKEN"
  type: "hosting"  # "hosting" (EUR) или "local" (RUB)
